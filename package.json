{
  "name": "jroze",
  "version": "1.0.0",
  "description": "Jano Roze Port",
  "engines": {
    "node": "8.11.2",
    "npm": "6.4.1"
  },
  "main": "server.js",
  "scripts": {
    "analyze": "source-map-explorer client/build/static/js/main.*",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Jano Roze",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "mongoose": "^5.3.14",
    "nodemailer": "^4.7.0",
    "path": "^0.12.7",
    "popper.js": "^1.14.6",
    "react": "^16.6.3",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-device-detect": "^1.6.2",
    "react-dom": "^16.8.1",
    "react-mail-form": "^1.0.2",
    "react-router-dom": "^4.3.1",
    "source-map-explorer": "^1.6.0",
    "twilio": "^3.39.2",
    "twilio-video": "^2.0.0-beta4",
    "yarn": "^1.12.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jroze88/TourneyReporting.git"
  },
  "bugs": {
    "url": "https://github.com/Jroze88/TourneyReporting/issues"
  },
  "homepage": "https://github.com/Jroze88/TourneyReporting#readme"
}
